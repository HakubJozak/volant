		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="bic_type">
		<xs:restriction base="xs:token">
			<xs:pattern value="[A-Z]{6}[A-Z0-9]{2}([A-Z0-9]{3})?"/>
		</xs:restriction>
	</xs:simpleType>


