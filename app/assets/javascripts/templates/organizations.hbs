{{ partial 'pagination' }}

<table class="table table-striped table-hover">
  <thead>
    <th>Country</th>
    <th>Name</th>
    <th>Emails</th>
    <th>Partners</th>
  </thead>
  <tbody>
    {{#each}}
      <tr {{action 'go_to_detail' this }}>
        <td>
          {{flag country}}
          {{country.name}}
        </td>
        <td>{{name}}</td>
        <td>{{# each email_contacts}}
            {{address}},
          {{/each}}
        </td>
        <td>
          {{# each email_contacts}}
            {{address}},
          {{/each}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>