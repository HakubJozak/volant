<h1>Volant</h1>

<table class="pure-table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Country</th>
      <th>Organization</th>
    </tr>
  </thead>
  <tbody>
    {{#each}}
      <tr>
        <td>{{name}}</td>
        <td>{{code}}</td>
        <td>{{country.name}}</td>
        <td>{{organization.name}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>



{{outlet}}
